{
  "extends": ["@rocketseat/eslint-config/node", "eslint:recommended", "plugin:import/errors", "plugin:import/warnings"],
  "plugins": ["import"],
  "rules": {
    "import/order": ["error", {"alphabetize": {"order": "asc"}}],
    "camelcase": "off",
    "no-useless-constructor": "off",
    "no-undef": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  }
}